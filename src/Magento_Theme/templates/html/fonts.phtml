<?php
/**
 * Module for asynchronous fonts CSS loading which speeds up first page render.
 * Just add path to your desired stylesheet to the array below.
 */
?>
<script>
    [
        'https://fonts.googleapis.com/css?family=Montserrat:300,400,700',
    ].forEach( function( url ) {
            loadCSS( url );
        } );
</script>
